{
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "lint:all": "tsslint --vue-project ./packages/*/tsconfig.json",
    "lint:root": "tsslint --vue-project ./tsconfig.json",
    "lint:app": "tsslint --vue-project ./packages/app/tsconfig.json",
    "lint:admin": "tsslint --vue-project ./packages/admin/tsconfig.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "node node_modules/@tsslint/eslint/scripts/generateDts.js"
  },
  "packageManager": "pnpm@10.17.1",
  "browserslist": [
    "defaults",
    "not dead",
    "not ie >= 0",
    "not ie_mob >= 0",
    "not op_mini all",
    "not KaiOS >= 0",
    "iOS >= 13.4",
    "Safari >= 13.1",
    "Chrome >= 74"
  ],
  "devDependencies": {
    "@koddsson/eslint-plugin-tscompat": "^0.2.0",
    "@tsslint/cli": "^2.0.4",
    "@tsslint/config": "^2.0.4",
    "@tsslint/eslint": "^2.0.4"
  }
}
